{
  "manifest_version": 2,

  "name": "IP",
  "description": "This extension grabs your public IP address from http://icanhazip.com",
  "version": "1.2",

  "browser_action": {
    "default_title": "Public IP",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://icanhazip.com/"
  ]
}